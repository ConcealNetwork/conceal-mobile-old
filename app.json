{
  "expo": {
    "name": "Conceal Mobile",
    "slug": "conceal_mobile",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "3.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#232323"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "enabled": false
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ConcealNetwork.ConcealMobile"
    },
    "android": {
      "package": "com.ConcealNetwork.ConcealMobile",
      "versionCode": 28,
      "permissions": [
        "CAMERA",
        "USE_FINGERPRINT"
      ]
    },
    "androidStatusBar": {
      "backgroundColor": "#212529",
      "translucent": false
    },
    "description": "Conceal Mobile Wallet"
  }
}
